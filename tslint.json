{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-a11y"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "jsx-double"
    ],
    "import-sources-order": "any",
    "object-literal-sort-keys": false,
    "react-a11y-lang": false,
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "ordered-imports": false,
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "interface-name": [
    "never-prefix"
  ],
  "rulesDirectory": []
}
